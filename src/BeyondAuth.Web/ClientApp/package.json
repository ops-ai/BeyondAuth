{
  "name": "beyondauth.web",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "conventional-changelog": "conventional-changelog",
    "start": "ng serve",
    "build": "ng build",
    "build:ssr": "ng run BeyondAuth.Web:server:dev",
    "build:prod": "npm run build -- --prod --aot",
    "test": "npm run install-puppeteer && ng test",
    "test:coverage": "rimraf coverage && npm run test -- --code-coverage --watch=false",
    "install-puppeteer": "cd node_modules/puppeteer && npm run install",
    "lint": "ng lint",
    "lint:fix": "ng lint beyondauth.web --fix",
    "lint:styles": "stylelint ./src/**/*.scss",
    "lint:ci": "npm run lint && npm run lint:styles",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "ng e2e",
    "docs": "compodoc -p src/tsconfig.app.json -d docs",
    "docs:serve": "compodoc -p src/tsconfig.app.json -d docs -s",
    "prepush": "npm run lint:ci",
    "release:changelog": "npm run conventional-changelog -- -p angular -i CHANGELOG.md -s",
    "postinstall": "ngcc --properties es2015 es5 browser module main --first-only --create-ivy-entry-points --tsconfig \"./src/tsconfig.app.json\""
  },
  "private": true,
  "dependencies": {
    "@akveo/ng2-completer": "^9.0.1",
    "@angular/animations": "^11.0.9",
    "@angular/cdk": "11.0.4",
    "@angular/common": "^11.0.9",
    "@angular/compiler": "^11.0.9",
    "@angular/core": "^11.0.9",
    "@angular/forms": "^11.0.9",
    "@angular/google-maps": "^11.0.4",
    "@angular/material": "11.0.4",
    "@angular/platform-browser": "^11.0.9",
    "@angular/platform-browser-dynamic": "^11.0.9",
    "@angular/router": "^11.0.9",
    "@asymmetrik/ngx-leaflet": "3.0.1",
    "@nebular/auth": "7.0.0",
    "@nebular/eva-icons": "7.0.0",
    "@nebular/security": "7.0.0",
    "@nebular/theme": "7.0.0",
    "@swimlane/ngx-charts": "^18.0.1",
    "angular-auth-oidc-client": "^10.0.15",
    "angular2-chartjs": "0.4.1",
    "bootstrap": "4.3.1",
    "chart.js": "3.3.0",
    "classlist.js": "1.1.20150312",
    "core-js": "2.5.1",
    "echarts": "4.6.0",
    "eva-icons": "^1.1.3",
    "intl": "1.2.5",
    "ionicons": "2.0.1",
    "jsnlog": "2.29.0",
    "leaflet": "1.2.0",
    "nebular-icons": "1.1.0",
    "ng2-completer": "3.0.3",
    "ng2-smart-table": "^1.6.0",
    "ngx-echarts": "4.2.2",
    "normalize.css": "6.0.0",
    "oidc-client": "^1.10.1",
    "pace-js": "1.0.2",
    "popper.js": "^1.16.1",
    "roboto-fontface": "0.8.0",
    "rxjs": "6.6.2",
    "rxjs-compat": "6.3.0",
    "socicon": "3.0.5",
    "tinymce": "^5.7.1",
    "tslib": "^2.0.0",
    "typeface-exo": "0.0.22",
    "web-animations-js": "^2.3.2",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1100.7",
